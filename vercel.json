{
  "version": 2,
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist/public",
  "installCommand": "pnpm install",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/trpc/:path*",
      "destination": "/api/index"
    },
    {
      "source": "/api/upload",
      "destination": "/api/index"
    },
    {
      "source": "/api/rss",
      "destination": "/api/index"
    },
    {
      "source": "/api/atom",
      "destination": "/api/index"
    },
    {
      "source": "/api/oauth/callback",
      "destination": "/api/index"
    }
  ]
}
